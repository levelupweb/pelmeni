{
    "compilerOptions": {
      "target": "es6",
      "module": "es6",
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./",
      "paths": {
        "@components/*": ["src/components/*"],
        "@root/*": ["/*"],
        "@pages/*": ["src/pages/*"],
        "@consts/*": ["src/consts/*"],
        "@variables/*": ["src/variabls/*"],
        "@HOC/*": ["src/HOC/*"],
        "@utils/*": ["utils/*"],
        "@API/*": ["src/API/*"],
        "@src/*": ["src/*"]
      },
      "jsx": "react"
    },
    "exclude": ["node_modules", "dist", ".git"]
  }
  